package com.cg.oopsinjava.gettersetter;

public class Shape {

	String colour="red";
	boolean filled=true;
	
	public String getColour() {
		return colour;
	}
	public void setColour(String colour) {
		this.colour = colour;
	}
	public boolean isFilled() {
		return filled;
	}
	public void setFilled(boolean filled) {
		this.filled = filled;
	}
	
}
