package edunextallprograms;

import java.util.Scanner;

class Utility
	
	public  FahrenheitToCelsius() {
		Scanner sc=new Scanner(System.in);
		double f;
		double cel;
		System.out.println("Enter temperature in Fehrenheit");
		f=sc.nextDouble();
		cel=(f-32)*5/9;
		//double roundDb1=Math.round(cel*100.0)/100.0;
		//int c=(int)roundDb1;
		System.out.println("\n Fahrenheit is :"+f);
		System.out.println("\n celcius is :"+Math.ceil(cel));
	}
	public class Source {
	
	public static void main(String[] args) {
		

	}
	

}
