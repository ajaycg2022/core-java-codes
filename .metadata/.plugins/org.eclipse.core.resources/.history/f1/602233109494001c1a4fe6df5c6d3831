package arraypractice;
import java.util.*;
import java.io.*;

public class OddEvenIndexSum {


	  public static void mOddEvenIndex(int array[]) {
	     //your code here
	  }

	  public static void main(String args[]) throws Exception {

	    int mSize = 10;
	    int[] Array = new int[mSize];
	    for (int i = 0; i < mSize; i++) {
	      Array[i] = i*i;
	    }

	    mOddEvenIndex(Array);

	  }
}
	
