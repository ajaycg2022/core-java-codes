package com.cg.oopsinjava.gettersetter;

public class Rectangle {

	double width=1.0;
	double length=1.0;
	Rectangle(){
	
	}
	public double getWidth() {
		return width;
	}
	public void setWidth(double width) {
		this.width = width;
	}
	public double getLength() {
		return length;
	}
	public void setLength(double length) {
		this.length = length;
	}
}
